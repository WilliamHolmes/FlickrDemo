{
    "presets": [
      [
        "@babel/preset-env", {
          "useBuiltIns": "entry",
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "compact": true,
    "env": {
      "development": {
        "sourceMaps": true,
        "plugins": [
          "react-hot-loader/babel"
        ]
      }
    },
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ]
  }